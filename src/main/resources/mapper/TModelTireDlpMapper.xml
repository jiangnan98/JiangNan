<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bing.jiang.mapper.TModelTireDlpMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.bing.jiang.entity.TModelTireDlp">
    <result column="id" property="id" />
        <result column="letter" property="letter" />
        <result column="brand" property="brand" />
        <result column="model" property="model" />
        <result column="category" property="category" />
        <result column="year" property="year" />
        <result column="tire" property="tire" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
    id,
        letter, brand, model, category, year, tire
    </sql>

</mapper>
